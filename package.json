{
  "name": "gist-clone",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc --build --clean && tsc",
    "lint": "eslint src/**/*.ts",
    "start": "npm run lint && npm run build && npm run dev",
    "test": "npm run build && ava",
    "dev": "node ./dist/src/index.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.10.1",
    "typeorm": "^0.3.12",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "ava": "^5.2.0",
    "eslint": "^8.34.0",
    "fs-extra": "^11.1.0",
    "typescript": "~4.9.5"
  },
  "ava": {
    "files": [
      "test/*"
    ],
    "timeout": "1m",
    "typescript": {
      "rewritePaths": {
        "test/": "dist/test/"
      }
    }
  }
}